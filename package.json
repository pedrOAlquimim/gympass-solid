{
  "name": "03-api-solid",
  "version": "1.0.0",
  "description": "Gympass style app",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:e2e": "vitest run --dir src/http",
    "pretest:e2e": "run-s test:crete-test-environment test:install-test-environment",
    "test:crete-test-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-test-environment": "npm link vitest-environment-prisma",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "Pedro Alquimim",
  "license": "ISC",
  "dependencies": {
    "@fastify/jwt": "7.2.0",
    "@prisma/client": "5.1.1",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "fastify": "4.21.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "20.4.9",
    "@vitest/coverage-v8": "0.34.2",
    "@vitest/ui": "0.34.2",
    "npm-run-all": "4.1.5",
    "prisma": "5.1.1",
    "tsup": "7.2.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.33.0"
  }
}
